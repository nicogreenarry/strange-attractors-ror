<div>
  <%= react_component("Attractor/index", { coefficients: attractor.details["coefficients"],
                                           startXy: attractor.details["startXy"],
                                           showEquation: false,
                                           initialCount: 30000,
                                           width: 300, height: 300 }) %>
  <p class="user"><%= link_to attractor.user.name, attractor.user %></p>
  <p class="timestamp">
    Discovered <%= time_ago_in_words(attractor.created_at) %> ago.
  </p>
</div>
